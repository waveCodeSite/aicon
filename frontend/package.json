{
  "name": "aicg-frontend",
  "version": "1.0.0",
  "description": "AICG内容分发平台前端",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:wsl": "npx vitest run --config vitest.direct.config.js",
    "test:basic": "npx vitest run --config vitest.direct.config.js",
    "test:vue": "npx vitest run --config vitest.minimal.config.js",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "axios": "^1.6.0",
    "element-plus": "^2.4.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.2.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
